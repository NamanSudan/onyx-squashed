# Basic Auth Settings
AUTH_TYPE=disabled
DISABLE_AUTH=true

# Database Settings
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=onyx
POSTGRES_HOST=relational_db

# Web Settings
WEB_DOMAIN=http://localhost:3003

# Proxy Settings
HTTP_PROXY=http://your.proxy.com:port
HTTPS_PROXY=http://your.proxy.com:port
NO_PROXY=localhost,127.0.0.1,relational_db,index,cache,inference_model_server,indexing_model_server,api_server,web_server,nginx

# Disable telemetry for local development
DISABLE_TELEMETRY=true

# Log Settings
LOG_LEVEL=info

# Gen AI Settings (optional, can be configured later)
DISABLE_GENERATIVE_AI=true  # Set to false and configure GEN_AI_API_KEY if you want to use Gen AI features
# GEN_AI_API_KEY=your_api_key_here 